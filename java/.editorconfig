#
#  http://editorConfig.org
#

root = true

[*]
indent_style             = space
end_of_line              = lf
charset                  = utf-8
trim_trailing_whitespace = true
insert_final_newline     = true

[*.{groovy,java,gradle}]
indent_style = space
indent_size  = 2

[*.md]
indent_size = 4
trim_trailing_whitespace = false

